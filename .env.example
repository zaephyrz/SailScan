# ============================================
# SAILSCAN - EXAMPLE ENVIRONMENT VARIABLES
# Copy to .env and fill in your values
# ============================================

# Flask
SECRET_KEY=change-this-to-a-random-secret-key
FLASK_DEBUG=True

# Database
DATABASE_URL=sqlite:///sailscan.db

# File Uploads
UPLOAD_FOLDER=uploads

# VirusTotal API (REQUIRED)
# Get free API key: https://virustotal.com/gui/join-us
VIRUSTOTAL_API_KEY=your_virustotal_api_key_here

# Frida (Optional - for dynamic analysis)
FRIDA_ENABLED=False

# App Settings
APP_NAME=SailScan
APP_VERSION=2.0.0